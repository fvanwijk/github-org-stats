<div id="org_repos_card" class="animated flipInY stackable-card mdl-card mdl-shadow--2dp">
  <div class="mdl-card__title">
    <h2 class="mdl-card__title-text">{{reposCount}} Repositories</h2>
  </div>
  <div class="mdl-card__supporting-text">
    Most <em>active</em> repos: <br /><br />
    {{#each repos}} 
      <div>
        <a href="#repos/{{owner.login}}/{{name}}">{{name}}</a>
        <span style="font-size:x-small">({{formatNumber open_issues_count}} issues)</span>
      </div>
    {{/each}}
  </div>
  <div class="mdl-card__actions mdl-card--border">
    <a href="/#orgs/{{orgName}}/repos" class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
      All Repositories
    </a>
  </div>
</div>

